#include "tokens.h"

const char *token_string(const struct token *t) {
    switch (t->type) {
        {% for name, string in TOKENS.iteritems() -%}
        case {{name}}: return "{{string}}";
        {% endfor -%}
        default: return "unknown symbol";
    }
}
