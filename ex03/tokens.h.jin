#ifndef TOKENS_H
#define TOKENS_H

enum token_type {
    {% for name in TOKENS -%}
    {{name}},
    {% endfor %}
};

struct token {
    enum token_type type;
    char *text;
    unsigned int line;
};

/* convert given token to string */
const char *token_string(const struct token *t);

#endif
